{
    "compilerOptions": {
        "allowJs": true,
        "target": "es6",
        "moduleResolution": "node",
        "sourceMap": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "outDir": "dist",
        "jsx": "react",
        "baseUrl": ".",
        "paths": {
            "utils": [
                "./src/utils.ts"
            ],
            "styles": [
                "./src/assets/style/"
            ],
            "models": [
                "./src/stores/models/"
            ]
        }
    },
    "exclude": [
        "node_modules"
    ]
}